    "a = int(input())\n",
    "b = a\n",
    "n = 0\n",
    "while b != 0:\n",
    "    b = b // 10\n",
    "    n += 1\n",
    "p = 0\n",
    "for i in range(n):\n",
    "    k = ((a//(10**(n-i-1)))%10)\n",
    "    p = p + k*(10**i)\n",
    "print (p)"
   ]
